{
    "extends": ["airbnb"],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "no-case-declarations": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": ["Link"],
          "specialLink": ["to", "hrefLeft", "hrefRight"],
          "aspects": ["noHref", "invalidHref", "preferButton"]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {}
      }
    }
  }